{
  "name": "react-assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.25.9",
    "@types/express": "^5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "express": "5.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.30.0",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "yarn react-scripts start",
    "start:backend": "nodemon --watch src --ext ts,js,json --exec 'ts-node ./src/server.ts'",
    "build": "cross-env CI=true yarn react-scripts build",
    "build:check": "tsc --noEmit && cross-env CI=true yarn react-scripts build",
    "test": "cross-env CI=true yarn react-scripts test",
    "eject": "yarn react-scripts eject",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "prepare": "husky",
    "docker-build": "docker build -t assignment-backend .",
    "docker-run": "docker run -v ./:/app -p 3000:3000 -t assignment-backend yarn start:backend"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "cross-env": "7.0.3",
    "eslint": "9.24.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  },
  "type": "module"
}